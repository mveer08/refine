{
    "name": "@pankod/refine-antd-audit-log",
    "description": "refine audit log component for antd package. refine is a React-based framework for building internal tools, rapidly. It ships with Ant Design System, an enterprise-level UI toolkit.",
    "version": "3.27.2",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "private": true,
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsup --watch --dts --format esm,cjs,iife --legacy-output",
        "build": "tsup --dts --format esm,cjs,iife --minify --legacy-output",
        "prepare": "npm run build"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0"
    },
    "author": "Pankod",
    "module": "dist/esm/index.js",
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@types/json-stable-stringify": "^1.0.34",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3",
        "tslib": "^2.3.1",
        "tsup": "^5.11.13",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@pankod/refine-antd": "^3.35.4",
        "@pankod/refine-core": "^3.56.6",
        "dayjs": "^1.10.7",
        "json-stable-stringify": "^1.0.1",
        "react-diff-viewer": "^3.1.1",
        "@tanstack/react-query": "^4.0.10"
    },
    "gitHead": "829f5a516f98c06f666d6be3e6e6099c75c07719",
    "publishConfig": {
        "access": "public"
    },
    "tsup": {
        "entry": [
            "src/index.tsx"
        ],
        "splitting": false,
        "sourcemap": true,
        "clean": false,
        "platform": "browser"
    }
}